{
  "name": "bookingsync-calendar-widget",
  "version": "0.3.1",
  "description": "BookingSync Calendar Widget",
  "main": "dist/bookingsync-calendar-widget.js",
  "module": "src/bookingsync-calendar-widget.js",
  "scripts": {
    "dev": "webpack --env.NODE_ENV=development",
    "build": "webpack --env.NODE_ENV=production",
    "test": "karma start karma.config.js --colors --env.NODE_ENV=development",
    "test-ci": "karma start karma.config.js --single-run true --env.NODE_ENV=development",
    "start": "webpack-dev-server --inline --hot --env.NODE_ENV=development --host 0.0.0.0",
    "mock-server": "node json-server.js",
    "publish-please": "publish-please",
    "prepublishOnly": "publish-please guard",
    "preversion": "npm run test-ci",
    "version": "npm run build && npm run dev && git add -A dist",
    "postversion": "git push && git push --tags && rm -rf build/temp"
  },
  "dependencies": {
    "moment": "^2.22.2",
    "sinon": "6.1.4",
    "tether-drop": "1.4.2",
    "widget-utils": "0.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.1",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.2",
    "chai": "^4.1.2",
    "css-loader": "^1.0.0",
    "eslint": "^4.19.1",
    "eslint-loader": "2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "json-server": "0.14.0",
    "karma": "^3.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "3.0.0",
    "mocha": "5.2.0",
    "node-sass": "^4.5.3",
    "publish-please": "^3.2.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.21.0",
    "tether": "1.4.4",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "3.1.5",
    "yargs": "12.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BookingSync/calendar-widget.git"
  },
  "author": "BookingSync.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BookingSync/calendar-widget/issues"
  },
  "homepage": "https://github.com/BookingSync/calendar-widget#readme",
  "engines": {
    "yarn": "YARN NO LONGER USED - use npm instead."
  }
}
